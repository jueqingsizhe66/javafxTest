<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.layout.*?>
<GridPane opacity="0.66" prefHeight="465.0" prefWidth="575.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.qny.AlertController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Button lineSpacing="10.0" mnemonicParsing="false" onAction="#message" text="消息通知" GridPane.halignment="CENTER">
         <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </GridPane.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#message2" text="没有标题的消息通知框" GridPane.columnIndex="1" GridPane.halignment="CENTER">
         <GridPane.margin>
            <Insets left="10.0" />
         </GridPane.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#warning" text="警告" GridPane.halignment="CENTER" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
      </Button>
      <Button mnemonicParsing="false" onAction="#sure" text="确认对话框" GridPane.halignment="CENTER" GridPane.rowIndex="2">
         <GridPane.margin>
            <Insets top="20.0" />
         </GridPane.margin></Button>
      <Button mnemonicParsing="false" onAction="#error" text="错误" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
      <VBox prefHeight="151.0" prefWidth="167.0" spacing="10.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#sureDefined" prefHeight="23.0" prefWidth="104.0" text="自定义对话框" VBox.vgrow="ALWAYS" />
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#inputDialog" prefHeight="23.0" prefWidth="106.0" text="可输入对话框" textFill="#a41717" />
            <Button layoutX="10.0" layoutY="33.0" mnemonicParsing="false" onAction="#choiceDialog" prefHeight="23.0" prefWidth="108.0" text="选择对话框" />
         </children>
         <GridPane.margin>
            <Insets left="40.0" top="30.0" />
         </GridPane.margin>
      </VBox>
      <HBox GridPane.rowIndex="3">
         <children>
            <Button mnemonicParsing="false" onAction="#loginDialog" text="登录">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#display" text="自定义消息对话框" />
         </children>
      </HBox>
      <Button mnemonicParsing="false" onAction="#switchToPrimary" text="切换为主界面" GridPane.columnIndex="1" GridPane.rowIndex="3" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <effect>
      <SepiaTone />
   </effect>
</GridPane>
